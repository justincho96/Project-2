<%- include('../partials/header') %>
  <title>My Workouts</title>
</head>
<body>
  <h1>Workout</h1>
  <table>
    <thead>
      <tr>
        <th>Exercise</th>
        <th>Repitions</th>
        <th>Complete</th>
      </tr>
    </thead>
    <tbody>
      <% workouts.forEach(w=> { %>
        <tr>
            <td><%= w.Exercise %></td>
            <td><%= w.Reps %></td>
            <td><%= w.complete ? 'Yes' : 'No' %></td>
            </tr>
            <% }) %>
    </tbody>
  </table>

<%- include('../partials/footer') %>

